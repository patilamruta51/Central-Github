pipeline 
{
    agent { node { label 'built-in' } } 
parameters {
        string(name: 'PERSON', defaultValue: 'Mr Jenkins', description: 'Whom should I say hello to?')
    }
stages 
	{
        stage('Example') 
		{
            steps 
			{
                echo "Hello ${params.PERSON}"
                }
        }
        stage('Bye')
		{
            steps 
			{
                echo 'Good Bye'
                
            }
        }
     
      
    }
}
